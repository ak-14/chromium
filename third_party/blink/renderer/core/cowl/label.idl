// https://w3c.github.io/webappsec-cowl

[
    Constructor,
    Constructor(DOMString principal),
    RaisesException=Constructor
] interface Label {
    boolean equals(Label other);
    boolean subsumes(Label other, optional Privilege priv);

    [ImplementedAs=and_] Label and(Label other);
    [ImplementedAs=and_, RaisesException] Label and(DOMString principal);

    [ImplementedAs=or_] Label _or(Label other);
    [ImplementedAs=or_, RaisesException] Label _or(DOMString principal);

    stringifier;
};
